{
  "name": "@wrelik/jobs",
  "version": "0.1.2",
  "publishConfig": {
    "access": "public"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/lwhite702/wrelik-kit.git",
    "directory": "packages/jobs"
  },
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    },
    "./server": {
      "types": "./dist/server.d.ts",
      "import": "./dist/server.js",
      "require": "./dist/server.cjs"
    },
    "./shared": {
      "types": "./dist/shared.d.ts",
      "import": "./dist/shared.js",
      "require": "./dist/shared.cjs"
    },
    "./react-native": {
      "types": "./dist/unsupported.d.ts",
      "import": "./dist/unsupported.js",
      "require": "./dist/unsupported.cjs"
    }
  },
  "react-native": "./dist/unsupported.js",
  "scripts": {
    "build": "tsup src/index.ts src/server.ts src/shared.ts src/unsupported.ts --format cjs,esm --dts --clean",
    "lint": "eslint src/",
    "test": "vitest run --passWithNoTests"
  },
  "dependencies": {
    "inngest": "^3.11.0"
  },
  "devDependencies": {
    "@wrelik/eslint-config": "workspace:*",
    "@wrelik/tsconfig": "workspace:*",
    "tsup": "^8.0.1",
    "vitest": "^1.2.2"
  }
}
